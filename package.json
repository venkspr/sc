{
  "name": "qc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "eslint": "eslint \"src/**/*.js\"",
    "format": "prettier-eslint \"src/**/*.js\" \"src/**/*.scss\" --write --trailing-comma es6",
    "dev": "nodemon --exec babel-node src/server/server.js",
    "debug": "babel-node --inspect src/server/server.js",
    "webpack": "webpack -wd",
    "start": "concurrently \"npm run webpack\" \"npm run dev\"",
    "test": "jest",
    "verify-tests": "jest --coverage",
    "build-react": "cross-env NODE_ENV=production webpack --progress -p",
    "build-node": "babel src -d build --copy-files",
    "build": "npm install && npm run build-react && npm run build-node",
    "prod": "cross-env NODE_ENV=production NODE_PATH=./build pm2 start -i max build/server/server.js --update-env --name qcProd",
    "prod-stop": "pm2 stop qcProd",
    "prod-reload": "pm2 reload --update-env qcProd",
    "prod-logs": "pm2 logs qcProd"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.19.0",
    "cross-env": "^5.2.1",
    "css-loader": "^0.28.11",
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "jbox": "^0.4.8",
    "mdbootstrap": "^4.8.10",
    "minimatch": "^3.0.4",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "node-sass": "^4.12.0",
    "pm2": "^2.10.4",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-hint": "^3.2.0",
    "react-loader-advanced": "^1.7.1",
    "react-router": "^3.2.4",
    "react-router-dom": "^4.3.1",
    "react-spinkit": "^3.0.0",
    "react-toastr": "^3.0.0",
    "react-tooltip": "^3.11.1",
    "reactstrap": "^5.0.0",
    "round-to": "^2.0.0",
    "sass-loader": "^6.0.7",
    "save": "^2.4.0",
    "style-loader": "^0.20.3",
    "surge": "^0.20.5",
    "toastr": "^2.1.4"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "concurrently": "^3.6.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.16.0",
    "jest": "^22.4.4",
    "nodemon": "^1.19.4",
    "prettier-eslint-cli": "^4.7.1",
    "react-test-renderer": "^16.10.2",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^3.12.0",
    "webpack-merge": "^4.2.2"
  }
}
